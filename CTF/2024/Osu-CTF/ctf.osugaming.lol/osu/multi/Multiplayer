                    placeholder="password"
                    required
                />
            </div>

                            <div class="login-box__row">
                    <div
                        class="js-captcha--container"
                        data-captcha-triggered=""
                    ></div>
                </div>
                <script>
    osuCore.turbolinksReload.load('https://www.recaptcha.net/recaptcha/api.js?render=explicit&onload=initCaptcha&hl=en');
    function initCaptcha() { osuCore.captcha.init('6LcsMMEZAAAAAEaveJ7bCI5sizQVYol-dAGVZFoq'); }
</script>
            
